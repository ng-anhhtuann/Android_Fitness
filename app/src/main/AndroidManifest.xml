<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pro.fitnessappproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DontEatActivity"></activity>
        <activity android:name=".DietActivity" />
        <activity android:name=".LevelActivity" />
        <activity android:name=".GoalWeightActivity" />
        <activity android:name=".WeightActivity" />
        <activity android:name=".HeightActivity" />
        <activity android:name=".AgeActivity" />
        <activity android:name=".MotivateActivity" />
        <activity android:name=".BodyAimActivity" />
        <activity android:name=".SexChoosingActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>